<header>
  <h3>Hello <%= current_user.first_name %>  edit your profile here</h3>
</header>

  <div class="col-xs-12 col-sm-6 col-md-3 display-user">
    <div class="display-registration">
      <div class="registration-description">
        <p class="registration-name"><%= @user.first_name %> <%= @user.last_name %></p>
        <p class="registration-name"><%= @user.user_name %></p>

      </div>
      <div class="registration-links">
        <span><%= link_to 'Show', current_user %></span>
        <span><%= link_to 'Edit', edit_user_path(current_user) %></span>
        <span><%= link_to 'Delete', current_user, method: :delete, data: { confirm: 'Are you sure?' } %></span>
      </div>
    </div>
  </div> <!-- /col -->
